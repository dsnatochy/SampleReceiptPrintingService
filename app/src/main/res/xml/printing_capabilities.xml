<?xml version="1.0" encoding="utf-8"?>
<capability>
    <!-- Special App ID for Transaction Processing-->
    <appid>co.poynt.samples</appid>

    <type>RECEIPT</type>

    <!-- Attribute for the printer could be one of the following
        name: subtype:
        value: one of the following.
            RECEIPT -> This printer is used for receipt printing.
            KITCHEN -> This printer is used for kitchen printing.
            OTHER -> other sub types..


        -->

    <attribute name="subtype" value="RECEIPT" />

    <!-- descriptive name of this capability -->
    <provider>Sample Receipt Service</provider>>


</capability>